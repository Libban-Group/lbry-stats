<script is:inline>
  function map(nodes) {
    const countries = {};

    nodes.forEach(node=>{
      if (!countries[node.geo.country.toLowerCase().replaceAll(' ', '_')]) countries[node.geo.country.toLowerCase().replaceAll(' ', '_')] = [];
      countries[node.geo.country.toLowerCase().replaceAll(' ', '_')].push(node);
    });

    Object.keys(countries).forEach(c=>{
      try {
        const country = document.querySelector(`.${c}`);
        country.classList.add('active');
      } catch(err) {
        console.warn(`Seems like ${c} is not a valid country on the map.`);
      }
    });
  }
</script>
<style>
  #map .country {
    fill: rgba(0, 0, 0, 0);
    stroke: #fff;
    stroke-width: 0.5px;
    transition: 0.2s;
    opacity: 1;
    cursor: pointer;
  }

  #map .country:hover {
    fill: rgba(0, 0, 0, 0.2);
    opacity: 0.5;
  }

  #map .active, #map .active:hover {
    fill: #FFA500;
  }
</style>